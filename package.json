{
  "name": "swapdiff",
  "displayName": "swapdiff",
  "description": "Swap diff documents",
  "version": "0.2.2",
  "publisher": "shalimski",
  "license": "SEE LICENSE IN LICENSE.md",
  "icon": "images/icon.png",
  "keywords": [
    "swap",
    "diff"
  ],
  "bugs": {
    "url": "https://github.com/shalimski/swapdiff/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shalimski/swapdiff.git"
  },
  "homepage": "https://github.com/shalimski/swapdiff/blob/master/README.md",
  "engines": {
    "vscode": "^1.79.1"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "swapdiff.swapdiff",
        "title": "Swap diff documents",
        "icon": "$(arrow-swap)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "swapdiff.swapdiff",
          "when": "isInDiffEditor",
          "group": "navigation"
        }
      ]
    },
    "configuration": [
      {
        "title": "swapdiff",
        "properties": {
          "swapdiff.ClosePreviousDiff": {
            "type": "boolean",
            "default": false,
            "description": "Close previous diff after switching",
            "scope": "window"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.8.3",
    "@types/vscode": "^1.79.1",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.43.0",
    "glob": "^10.3.6",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "vscode-test": "^1.6.1"
  }
}
